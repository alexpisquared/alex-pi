<UserControl x:Class="AzureLogParser.ClockUserControl" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:d="http://schemas.microsoft.com/expression/blend/2008"              mc:Ignorable="d" 
             xmlns:local="clr-namespace:AzureLogParser" x:Name="ths"
             d:DesignHeight="450" d:DesignWidth="800" Loaded="OnLoaded">
  <Viewbox>
    <TextBlock Text="{Binding ElementName=ths, Path=Now, StringFormat='{}{0:H:mm:ss.f}'}" d:Text="12:12:12.1" Foreground="#08f" />
  </Viewbox>
</UserControl>
